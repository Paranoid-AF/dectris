{
  "name": "dectris",
  "version": "0.0.1",
  "description": "Decky plugin that adds games from Lutris.",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "prepare": "pip install -t ./py_modules/externals -r requirements.txt"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Paranoid-AF/dectris.git"
  },
  "keywords": [
    "decky",
    "plugin",
    "steam-deck",
    "deck",
    "non-steam",
    "launchers",
    "lutris"
  ],
  "author": "Paranoid_AF <thatpaf@outlook.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/Paranoid-AF/dectris/issues"
  },
  "homepage": "https://github.com/Paranoid-AF/dectris#readme",
  "devDependencies": {
    "@decky/rollup": "^1.0.1",
    "@decky/ui": "^4.7.2",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/webpack": "^5.28.5",
    "rollup": "^4.22.5",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@decky/api": "^1.1.2",
    "react-icons": "^5.3.0",
    "tslib": "^2.7.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom"
      ]
    }
  }
}
